@model DetailStaffViewModel

@{
    ViewData["Title"] = "DetailStaff";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<form>
    <label>Staff ID</label>
    <input type="text" asp-for="StaffID" class="form-control" disabled />
    <br />
