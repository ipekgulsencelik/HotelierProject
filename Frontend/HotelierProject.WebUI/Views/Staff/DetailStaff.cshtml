    <input type="text" asp-for="Name" class="form-control" disabled />
    <br />
    <label>Staff Title</label>
    <input type="text" asp-for="Title" class="form-control" disabled />
    <br />
    <label>Social Media 1</label>
    <input type="text" asp-for="SocialMedia1" class="form-control" disabled />
    <br />
    <label>Social Media 2</label>
    <input type="text" asp-for="SocialMedia2" class="form-control" disabled />
    <br />
    <label>Social Media 3</label>
    <input type="text" asp-for="SocialMedia3" class="form-control" disabled />
